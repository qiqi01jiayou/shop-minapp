.container {
    box-sizing: border-box;
    padding: 30rpx;
    background-color: white;
}

.sku-preview {
    display: flex;
    flex-direction: row;
}

.l-price {
    margin-right: 10rpx;
}

.sku-img {
    width: 160rpx;
    height: 160rpx;
}

.sku-des {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 40rpx;
}

.sku-pending {
    font-size: 22rpx;
}

.price-row {
    display: flex;
    flex-direction: row;
    align-items: baseline;
}

.title {

}

.hr {
    width: 690rpx;
    height: 2rpx;
    background-color: rgb(233, 233, 233);
}

.stock {
    margin-left: 30rpx;
    font-size: 20rpx;
    color: #999;
}

.stock-pinch {
    color: #d20109;
    margin-left: 30rpx;
    font-size: 20rpx;
}
.sku-pending > text:first-child {
    color: #999;
}

.sku-pending > text:nth-child(2) {
    color: #666;
}