<l-loadmore show type="{{loadingType}}" >
    <view slot="content" class="container">
        <image class="top-theme" src="{{themeA.entrance_img}}"></image>
        <swiper class="swiper" indicator-dots indicator-active-color="#157658" autoplay circular>
            <block wx:for="{{bannerB.items}}" wx:key="index">
                <swiper-item>
                    <image class="swiper" src="{{item.img}}"></image>
                </swiper-item>
            </block>
        </swiper>
        <s-category-grid grid="{{gridC}}"></s-category-grid>
        <image class="activity" src="{{activityD.entrance_img}}"></image>
        <s-spu-scroll theme="{{themeE}}" spu-list="{{themeESpu}}" wx:if="{{themeE.online}}"></s-spu-scroll>
        <image src="{{themeF.entrance_img}}" class="quality"></image>
        <s-hot-list banner="{{bannerG}}"></s-hot-list>
        <image src="{{themeH.entrance_img}}" class="fashion"></image>
        <view class="spu-bottom">
            <image src="../../imgs/home/title@interest.png" class="title-spu-bottom"></image>
            <l-water-flow generic:l-water-flow-item="s-spu-preview"></l-water-flow>
        </view>
    </view>
</l-loadmore>